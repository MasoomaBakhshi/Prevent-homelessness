            <%}%>
          <h2><%- serve.organization %></h2>
          <strong class="note"><%= serve.note %></strong>
          <p><%- serve.body %></p>
          <% if (serve.phone){%>
          <div class="info">
            <strong><i class="fas fa-phone" aria-hidden="true"></i> Phone: </strong><a href="tel:<%- serve.phone %>" ><%- serve.phone %></a>
          </div>
          <%}%>
        <a href="<%- serve.forum %>" class="btn-primary btn-block" target="_blank">
          Forum </a>
      </div>
      <% } })%>
    </div>
    <div class="tab-pane fade" id="comunity" role="tabpanel" aria-labelledby="services-tab">
