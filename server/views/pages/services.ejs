          <%}%>
          <a
          href="<%- serve.address %>"
          class="btn-primary btn-block"
          target="_blank"
          >Locate </a
        >
      </div>
      <% }}) %>
    </div>
    <div class="tab-pane fade" id="chat" role="tabpanel" aria-labelledby="chat-tab">
      <% services.forEach(serve => { if (serve.mode.includes('chat')){%>
